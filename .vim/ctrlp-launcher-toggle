CtrlP Show Hidden	let g:ctrlp_show_hidden = !get(g:, 'ctrlp_show_hidden', 0) | echo (g:ctrlp_show_hidden ? 'show' : 'hidden') | CtrlPClearCache
Line Number	:LineNumberToggle
#Nrformats Alpha	:NrformatsAlphaToggle
paste	:set paste! paste?
syntax	:if exists('g:syntax_on') | syntax off | else | syntax enable | endif
wrap	:setlocal wrap! wrap?
scrolloff 999 <=> 5	:exe 'set scrolloff='.(&scrolloff == 999 ? '5' : '999').' scrolloff?'
SignifyToggleHighlight	:SignifyToggleHighlight
