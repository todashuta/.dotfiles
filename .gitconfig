# .gitconfig
# https://github.com/todashuta/profiles

[color]
	# colorの設定(自動で色付け)
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[alias]
	ss = status -sb
	sl = shortlog -sn
	wdiff = diff --word-diff
	unstage = reset HEAD --
[core]
	editor = vim
	#pager = lv -c
[merge]
	tool = vimdiff

#[push]
##	For Git Version 1.8
#	default = simple

# __END__
