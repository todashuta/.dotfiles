" .vimperatorrc

nnoremap : ;
nnoremap ; :

cnoremap <C-p>  <Up>
cnoremap <C-n>  <Down>

nnoremap <C-h>  :<C-u>help<Space>

nnoremap <Esc><Esc>  :<C-u>nohlsearch<CR>
nnoremap <C-[><C-[>  :<C-u>nohlsearch<CR>

set hintchars=hjklasdfgyuiopqwertnmzxcvb
set status=input,history,tabcount,position,bookmark,ssl
set titlestring=Firefox
"set gui=noaddons,nobookmarks
set gui+=tabs

" __END__
